{
    "manifest_version": 2,
    "name": "Phishing Guardian",
    "version": "1.0.0",
    "description": "Firefox phishing detection",
    "permissions": [
        "tabs",
        "storage",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"],
        "run_at": "document_start"
    }],
    "browser_action": {
        "default_icon": "assets/shield.png",
        "default_popup": "popup/popup.html"
    },
    "web_accessible_resources": [
        "redirect.html",
        "redirect.js",
        "assets/alert.png",
        "assets/shield.png",
        "assets/ui.css"
    ],
    "icons": {
        "16": "assets/shield.png",
        "48": "assets/shield.png",
        "128": "assets/shield.png"
    },
